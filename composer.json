{
    "name": "ironphp/ironphp",
    "description": "The IronPHP Framework",
    "homepage": "https://github.com/ironphp/ironphp",
    "type": "library",
    "keywords": [
        "php",
        "framework",
        "mvc",
        "ironphp",
        "frontcontroller",
        "iron-php",
        "ironphp-framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "GaurangKumar Parmar",
            "email": "gaurangkumarp@gmail.com",
            "homepage": "https://twitter.com/gaurangkumarp",
            "role": "Founder and Developer"
        }
    ],
    "support": {
        "email": "gaurangkumarp@gmail.com",
        "forum": "https://www.facebook.com/IronPHP-Framwork-325690624644002",
        "issues": "https://github.com/ironphp/ironphp/issues",
        "source": "https://github.com/ironphp/ironphp"
    },
    "autoload": {
        "psr-4": {
            "Friday\\": "src/Friday/"
        },
        "files": [
            "src/Friday/Helper/Function.php",
            "src/Friday/Helper/Constant.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "phing/phing-composer-configurator": true
        }
    },
    "require": {
        "erusev/parsedown": "dev-master",
        "guzzlehttp/guzzle": "7.4",
        "league/flysystem": "3.0",
        "monolog/monolog": "2.4",
        "psr/log": "3.0",
        "symfony/mailer": "6.0",
        "vlucas/phpdotenv": "5.4"
    },
    "require-dev": {
        "atoum/atoum": "4.0",
        "codeception/codeception": "@dev",
        "kahlan/kahlan": "5.2",
        "phing/phing": "@dev",
        "phpspec/phpspec": "7.2",
        "phpstan/phpstan": "1.5",
        "phpunit/phpunit": "^9.5"
    }
}
