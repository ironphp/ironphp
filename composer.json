    },
    "require-dev": {
        "atoum/atoum": "@stable",
        "codeception/codeception": "@stable",
        "datasift/storyplayer": "@stable",
        "designsecurity/progpilot": "@stable",
        "kahlan/kahlan": "@stable",
        "phing/phing": "@stable",
        "phpspec/phpspec": "@stable",
        "phpstan/phpstan": "@stable",
        "phpunit/phpunit": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Friday\\": "src/Friday/"
        },
        "files": [
            "src/Friday/Helper/Function.php",
            "src/Friday/Helper/Constant.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
